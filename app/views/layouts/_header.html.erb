<div class="navbar navbar-default navbar-fixed-top">
	<div class="container">
		<!-- made this div to hide when size at @768px... hides, but spacing still there -->
		<div class="test">
			<a class="brand hide-desktop"><%= link_to "Hacker Corral", root_url, id: "logo" %></a>
		</div>
    <button class="navbar-toggle" data-toggle="collapse" data-target=".navHeaderCollapse">
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
  </div>

  <div class = "collapse navbar-collapse navHeaderCollapse">
  <a class="brand hide-desktop"><%= link_to "Hacker Corral", root_url, id: "logo" %></a>
    <ul class="nav navbar-nav navbar-right">
			<li class= "active"><%= link_to "Home", root_url %></li>
			<li><%= link_to "Help", 'http://www.youtube.com/watch?v=x7Q5NRbQgCQ&list=PLD77C1EAA55A70072' %></li>
			<li><%= link_to "Events", event_path %></li>
			<!-- if the current user is signed in it will show username, else signin/up -->
			<% if current_user %>
				<li><a>Logged in as <%= current_user.username %></a></li>
				<li><%= link_to "Sign Out", signout_path %></li>
			<% else %>
				<!-- modals called from application -->
        <li data-toggle="modal" data-target="#signinModal"><%= link_to "Sign in", signin_path %></li>
				<li data-toggle="modal" data-target="#signupModal"><%= link_to "Sign up", signup_path %></li>
			<% end %>
		</ul>
	</div>
</div>



